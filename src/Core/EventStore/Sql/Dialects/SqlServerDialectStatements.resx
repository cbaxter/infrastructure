  <data name="DeleteStream" xml:space="preserve">
    <value>DELETE FROM [dbo].[Commit] WHERE [StreamId] = @streamId;</value>
  </data>
  <data name="EnsureCommitTableExists" xml:space="preserve">
    <value>IF NOT EXISTS (SELECT * FROM sys.sysobjects WHERE type = 'U' AND name = 'Commit' AND uid = SCHEMA_ID())
BEGIN
    CREATE TABLE [Commit] (
        [Id]        UNIQUEIDENTIFIER NOT NULL,
        [Sequence]  BIGINT           NOT NULL IDENTITY(1,1),
        [Timestamp] DATETIME2        NOT NULL,
        [StreamId]  UNIQUEIDENTIFIER NOT NULL,
        [Version]   INT              NOT NULL,
        [Headers]   VARBINARY(MAX)   NOT NULL,
        [Events]    VARBINARY(MAX)   NOT NULL
    );

    CREATE UNIQUE CLUSTERED INDEX [UQ_Commit_Sequence] ON [Commit]([Sequence]);
    CREATE UNIQUE NONCLUSTERED INDEX [UQ_Commit_StreamVersion] ON [Commit]([StreamId],[Version]);
END;</value>
  </data>
  <data name="EnsureDuplicateCommitsDetected" xml:space="preserve">
    <value>IF NOT EXISTS (SELECT * FROM sys.sysobjects WHERE xtype = 'PK' AND name = 'PK_Commit' AND uid = SCHEMA_ID())
BEGIN
    ALTER TABLE [Commit] ADD CONSTRAINT [PK_Commit] PRIMARY KEY NONCLUSTERED ([Id]);
END;</value>
  </data>
  <data name="EnsureDuplicateCommitsSuppressed" xml:space="preserve">
    <value>IF EXISTS (SELECT * FROM sys.sysobjects WHERE xtype = 'PK' AND name = 'PK_Commit' AND uid = SCHEMA_ID())
BEGIN
    ALTER TABLE [Commit] DROP CONSTRAINT [PK_Commit];
END;</value>
  </data>
  <data name="EnsureSnapshotTableExists" xml:space="preserve">
    <value>IF NOT EXISTS (SELECT * FROM sys.sysobjects WHERE xtype = 'U' AND name = 'Snapshot' AND uid = SCHEMA_ID())
BEGIN
    CREATE TABLE [Snapshot] (
        [StreamId]  UNIQUEIDENTIFIER NOT NULL,
        [Version]   INT              NOT NULL,
        [State]     VARBINARY(MAX)   NOT NULL,
     
        CONSTRAINT [PK_Snapshot] PRIMARY KEY CLUSTERED ([StreamId],[Version]) WITH (IGNORE_DUP_KEY = ON)
    );
END;</value>
  </data>
  <data name="GetRange" xml:space="preserve">
    <value>SELECT TOP (@take) [Id],[Sequence],[Timestamp],[StreamId],[Version],[Headers],[Events] FROM [dbo].[Commit] WHERE [Sequence] &gt;= @skip ORDER BY [Sequence];</value>
  </data>
  <data name="GetSnapshot" xml:space="preserve">
    <value>SELECT TOP(1) [StreamId],[Version],[State] FROM [dbo].[Snapshot] WHERE [StreamId] = @streamId AND [Version] &lt;= @version ORDER BY [Version] DESC;</value>
  </data>
  <data name="GetStream" xml:space="preserve">
    <value>SELECT TOP (@take) [Id],[Sequence],[Timestamp],[StreamId],[Version],[Headers],[Events] FROM [dbo].[Commit] WHERE [StreamId] = @streamId AND [Version] &gt;= @version ORDER BY [Version];</value>
  </data>
  <data name="GetStreams" xml:space="preserve">
    <value>SELECT TOP (@take) [StreamId] FROM [dbo].[Commit] WHERE [Version] = 0 AND [StreamId] &gt; @streamId ORDER BY [StreamId];</value>
  </data>
  <data name="InsertCommit" xml:space="preserve">
    <value>INSERT INTO [dbo].[Commit] ([Id], [Timestamp], [StreamId], [Version], [Headers], [Events]) OUTPUT Inserted.Sequence VALUES(@id, @timestamp, @streamId, @version, @headers, @events);</value>
  </data>
  <data name="InsertSnapshot" xml:space="preserve">
    <value>INSERT INTO [dbo].[Snapshot] ([StreamId],[Version],[State]) VALUES(@streamId, @version, @state);</value>
  </data>
  <data name="PurgeCommits" xml:space="preserve">
    <value>TRUNCATE TABLE [dbo].[Commit];</value>
  </data>
  <data name="PurgeSnapshots" xml:space="preserve">
    <value>TRUNCATE TABLE [dbo].[Snapshot];</value>
  </data>
  <data name="ReplaceSnapshot" xml:space="preserve">
    <value>UPDATE TOP(1) [dbo].[Snapshot] SET [Version] = @version, [State] = @state WHERE [StreamId] = @streamId;
IF @@ROWCOUNT = 0
BEGIN
    INSERT INTO [dbo].[Snapshot] ([StreamId],[Version],[State]) VALUES(@streamId, @version, @state);
END;</value>
  </data>
  <data name="UpdateCommit" xml:space="preserve">
    <value>UPDATE [dbo].[Commit] SET [Headers] = @headers, [Events] = @events WHERE [Id] = @id;</value>
